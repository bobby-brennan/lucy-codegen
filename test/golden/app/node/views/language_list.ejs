<% result.forEach(function(l, index) { %>
  <% if(l.foo) { %>
    <p>foo</p>
  <% } %>
  <% if(!l.foo) { %>
    <p>bar</p>
  <% } %>
  <% if(index != 2) { %>
    <h2><%- index %>. <%- l.label %></h2>
  <% } %>
  <% if(l.label == 'Ruby') { %>
    IF <%- l.label %>
    <% if(index == 0) { %>
      ZERO
    <% } else { %>
      NONZERO
    <% } %>
  <% } else { %>
    ELSE <%- l.label %>
    <% if(index == 0) { %>
      ZERO
    <% } else { %>
      NONZERO
    <% } %>
  <% } %>
<% }); %>
<br></br>
<br></br>
