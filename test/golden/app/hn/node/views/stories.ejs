<% result.forEach(function(storyID, index) { %>
  <% if(index == 0) { %>
    <div class="item"></div>
    <script>
      var element = $('.item:empty').first();
      element.html('loading...');
      element.load('getItem', {
        itemID: <%- JSON.stringify(storyID) %>,
      });
    </script>

  <% } else { %>
    <% if(index < 10) { %>
      <div class="story">
        <%- storyID %>Â 
        <a class="btn btn-success"
             data-action="getItem"
             data-view="itemView"
             data-answers="{itemID: <%- storyID %>}">
             Load
          </a>

      </div>
    <% } %>
  <% } %>
<% }); %>